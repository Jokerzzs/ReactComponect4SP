#news{
    width: 730px;
    height: 430px;
    padding: 15px 20px 20px 20px;


    .acs-webpartframe{
        .acs-webpartframe-header{
            .acs-header-icon{
                background-position: -40px 0;
            }
        }

         .acs-sliderframe {    
            position:relative;
            width:100%;
            overflow:hidden;

            > ul{
                position:relative;
                left:0;
                height:auto;
                overflow:hidden;
                transition: left 1s;
                padding: 0px;
                margin: 0px;
                border: 0px;
                height: 330px;

                > li{
                    display:inline-block;
                    height:auto;
                    height: 100%;
                    
                    > div{
                        display: flex;
                        flex-direction: column;
                        list-style: none;
                        height: 100%;

                        > div{
                            display: flex;
                            flex-direction:column;
                            flex-wrap: wrap;
                            border-bottom: 1px solid #e3e3e3;

                            &:nth-last-child(1) {
                                border-bottom: 0px;
                            } 

                            .newContent{
                                &:nth-child(1){
                                    width: 100px;
                                    height: 110px;
                                }
                                &:nth-child(2){
                                    width: calc(~"100% - 120px");
                                    height: 35px;
                                    margin-top:20px;
                                }
                                &:nth-child(3){
                                    width: calc(~"100% - 120px");
                                    height: 35px;
                                    color: #888;
                                    font-family: 'Frutiger CE 45 Light',
                                }

                                img{
                                    width: 100px;
                                    height: 70px;  
                                    margin-top:20px;                                
                                    border-radius: 4px;                              
                                }

                                a{
                                    color: #444;
                                    display: inline-block;
                                    vertical-align: middle;
                                    font-family: 'Frutiger CE 45 Light';
                                    font-family: 14px;
                                    text-decoration: underline;
                                }

                                span{
                                    font-size: 13px;
                                    margin-right: 5px;
                                    display: inline-block;
                                    margin-top: 12px;
                                }
                            }
                        }

                    }
                }
            }
        }
    }
}